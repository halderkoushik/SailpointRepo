<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE IdentityTrigger PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<IdentityTrigger disabled="true" handler="sailpoint.api.CertificationTriggerHandler" name="MOVER CERTIFICATION EVENT" type="Rule">
  <Description>Manager Mover Certification Trigger</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <HandlerParameters>
    <Attributes>
      <Map>
        <entry key="certificationDefinitionId" value="##MOVERCERTDEF001##"/>
      </Map>
    </Attributes>
  </HandlerParameters>
  <TriggerRule>
    <Reference class="sailpoint.object.Rule" name="Rule-IdentityTrigger-Mover"/>
  </TriggerRule>
</IdentityTrigger>